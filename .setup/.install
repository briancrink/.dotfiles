# Install Homebrew
usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"

# Install brew dependencies (use --file to specify a path different from cd)
if [ -f ~/.Brewfile ]; then
brew bundle --global;
fi


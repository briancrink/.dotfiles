# install imported apps (open audible, live home 3d pro, logic pro x)
    # `ls ~/Library/Mobile\ Documents/com~apple~CloudDocs/` is the location of iCloud Drive Document folders
    # `cp /Volumes/Drive/Workspace/Workflow/Setup/` is the location of Google Drive setup folders

cp -r ~/Library/Mobile\ Documents/iCloud~com~belightsoft~LiveHome3D/Documents/Live\ Home\ 3D\ Pro/Live\ Home\ 3D\ Pro.app /Applications/
# cp -r /Volumes/Drive/Workspace/Workflow/Setup/Live\ Home\ 3D\ Pro/Live\ Home\ 3D\ Pro.app /Applications/
cp -r ~/Library/Mobile\ Documents/com~apple~CloudDocs/Logic\ Pro\ X/Logic\ Pro\ X.app /Applications/
 cp -r ~/Library/Mobile\ Documents/com~apple~CloudDocs/Open\ Audible/OpenAudible.app /Applications/

# install fonts (system = /Library/Fonts | user = ~/Library/Fonts)
for font in ~/.dotfiles/.font/{avantgardepro,gotham,hack,operator-mono,sourcecodepro}/*.{otf,ttf}; do
  [ -f $font ] && cp $font ~/Library/Fonts;
done;
unset font;

# install font collections (Bug - does not copy collection with a space in the name)
for fontCollection in ~/.dotfiles/.font/FontCollections/*; do
  [ -f $fontCollection ] && cp $fontCollection ~/Library/FontCollections;
done;
unset fontCollection;


#! /usr/local/bin/bash

# # * Check for errors
brew doctor

# # * Clean up brew
brew cleanup

# # * Dot_clean
dot_clean -n $drive

# # * Empty the trash (empty, secure, skip confirmation)
trash -esy

# # * Reset folder .icns
fileicon set $ws $dot/.icns/workspace.icns
fileicon set $scanner $dot/.icns/scanner.icns
fileicon set $screenshot $dot/.icns/screenshot.icns
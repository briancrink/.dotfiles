#! /usr/local/bin/bash

# # * Folder .icns
fileicon get -f $downloads $dotfiles/.icns/downloads
fileicon get -f $scanner $dotfiles/.icns/scanner
fileicon get -f $screenshot $dotfiles/.icns/screenshot
fileicon get -f $workspace $dotfiles/.icns/workspace

# # * Brewfile (bundle = alias)
bundle

# # * VS Code
# install "Settings Sync" extension in vscode marketplace
# type in vscode command palette `Sync : Advance Options > Toggle Auto-Upload on Setting Change` to auto-backup
code --list-extensions > $dotfiles/.vscode/.vscode

# # * App data

# # * Finder View
# (com.apple.finder.plist -> convert to .. -> export to > ..)

# # * System Preferences
# (com.apple.***.plist -> ~convert to PlistBuddy bash commands~ -> ~export to~ -> ~/.sysprefs)

# # * Git commit / push

# # * Dotfiles Folder
# Send .dotfiles to.. -> Drive -> Workspace -> Mac -> dotfiles.zip
# Send .dotfiles to.. -> iCloud Drive -> Documents -> dotfiles.zip
zipdot

# # * Drive
# Send Workspace to.. -> iCloud Drive -> Documents -> workspace.zip
zipws

# # * ~prompt notification~
# (notication center -> backup has completed)

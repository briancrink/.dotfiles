#! /usr/local/bin/bash

# [Bash Escape Sequences](https://unix.stackexchange.com/questions/108635/why-i-cant-escape-spaces-on-a-bash-script)

# # * macOS Paths
dot=$HOME/.dotfiles # $dotfiles
downloads=$HOME/Downloads # $downloads
icloud=$HOME/Documents # $icloud

# # * Drive Paths
drive=/Volumes/Drive # $drive
ws=$drive/Workspace # $ws
doc=$ws/Document # $doc
setup=$ws/Setup # $setup
wf=$ws/Workflow # $wf
mac=$setup/Mac # $mac
scanner=$doc/Scanner # $scanner
screenshot=$doc/Screenshot # $screenshot
wallpaper=$setup/Wallpaper/Desktop # $wallpaper
light=$wallpaper/Light # $light
dark=$wallpaper/Dark # $dark
dusk=$wallpaper/Dusk # $dusk
theme=$wallpaper/Theme # $theme
neon=$theme/neonspacecandy # $neon
winter=$theme/winter # $winter
refind=$mac/Boot\ Manager/rEFInd # "$refind"
